		<%= csrf_meta_tags %>
		<%= content_for?(:head) ? yield(:head) : '' %>
		<%= html_import_tag 'application' %>
		<link rel="stylesheet" href="//cdn.materialdesignicons.com/1.1.70/css/materialdesignicons.min.css">
